# Environment variables for equation explanation server
PORT=3000

# API Keys - Replace with your actual API keys
GOOGLE_API_KEY=your_google_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Model configurations
OPENAI_MODEL=gpt-4.1-nano  # Options: gpt-4-turbo, gpt-4o, gpt-3.5-turbo, etc.
GEMINI_MODEL=gemini-2.5-flash-preview-05-20   # Options: gemini-2.5-flash-preview-05-20, gemini-pro, gemini-1.5-pro, etc.

# Output configurations
MAX_TOKENS=1500           # Maximum number of tokens in the output
TEMPERATURE=0.2           # Controls randomness (0.0-1.0)

# Add other configuration variables as needed
